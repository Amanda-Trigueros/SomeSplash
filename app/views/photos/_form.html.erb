<%= form_with model: photo do |form| %>
<div>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <% photo.errors.full_messages_for(:title).each do |message|  %>
    <span>
      <%= message %>
    </span>
  <% end %>
</div>
<div>
  <%= form.label :description %>
  <%= form.text_area :description%>

  <% photo.errors.full_messages_for(:description).each do |message|  %>
    <span>
      <%= message %>
    </span>
  <% end %>

</div>

<div>
  <%= form.submit %>
</div>

<% end %>